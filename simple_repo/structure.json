[{
    "package": "simple_repo.simple_sklearn",
    "module": "model_selection",
    "name": "TrainTestSplit",
    "input": [
        {
            "input_name": "dataset",
            "input_type": "pandas.DataFrame"
        }
    ],
    "parameters": [
        {
            "param_name": "train_size",
            "param_type": "float",
            "is_mandatory": true
        },
        {
            "param_name": "shuffle",
            "param_type": "bool"
        }
    ],
    "output": [
        {
            "output_name": "train_dataset",
            "output_type": "pandas.DataFrame"
        },
        {
            "output_name": "test_dataset",
            "output_type": "pandas.DataFrame"
        }
    ]
},
{
  "step_id": "split",
  "name": "sigmalib.transform.data_wrangling.SplitTrainTest",
  "attr": {
    "send_train_to": "zscore",
    "send_test_to": [
      "outtest",
      "zscorepred"
    ],
    "test_size": 0.3
  },
  "output": [
    {
      "result": "dataset_train",
      "send_to": "zscorepred",
      "as": "dataset"
    },
    {
      "result": "dataset_test",
      "send_to": "out",
      "as": "dataset"
    }
  ]
}]
